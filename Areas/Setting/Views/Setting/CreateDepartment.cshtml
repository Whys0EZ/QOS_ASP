@model QOS.Models.Team_List
@{
    Layout = "_MenuSetting";
    ViewData["Title"] = "Tạo nhà máy";
}

<div class=" me-3 ">
    <h2>Thêm nhà máy</h2>

    <form asp-action="CreateDepartment" method="post" class="mb-3">
        <div class="form-group mb-2">
            <label>TeamID</label>
            <input asp-for="TeamID" class="form-control" />
        </div>
        <div class="form-group mb-2">
            <label>TeamName</label>
            <input asp-for="TeamName" class="form-control" />
        </div>
        <div class="form-group mb-2">
            <label>Remark</label>
            <input asp-for="Remark" class="form-control" />
        </div>
        @* <div class="form-group">
            <label>Password</label>
            <input asp-for="Pass" type="password" class="form-control" />
        </div>
         *@

        
        
        <button type="submit" class="btn btn-success mt-4">Save</button>
        <a asp-action="DepartmentList" class="btn btn-secondary mt-4">Back</a>

    </form>
</div>